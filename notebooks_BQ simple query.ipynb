   "source": [
    "%%%sql\n",
    "SELECT\n",
    "  LANGUAGE,\n",
    "  SUM(views) AS views\n",
    "FROM\n",
    "  [bigquery-samples:wikipedia_benchmark.Wiki10B]\n",
    "WHERE\n",
    "  REGEXP_MATCH(title,\n",
    "    \"G.*o.*o.*g\")\n",
    "GROUP BY\n",
    "  LANGUAGE\n",
    "ORDER BY\n",
    "  views DESC;"
   ]
